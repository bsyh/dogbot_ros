<?xml version="1.0"?>


<robot name="dogbot" xmlns:xacro="http://www.ros.org/wiki/xacro">




<!-- pre-defined colors -->
<material name="tiffany">
    <color rgba="0.039 0.729 0.709 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  <material name="white_1">
    <color rgba="1 1 1 0.7"/>
  </material>


    <!-- body part -->
  <link name = "base_link">
   <visual>
    <geometry>
     <box size="1 0.3 0.2" />
    </geometry>

     <material name = "tiffany"/>
     <origin rpy="0 0 0" xyz="0 0 0.5"/>
    </visual>

    <collision>
     <origin rpy="0 0 0" xyz="0 0 0.5"/>
    <geometry>
     <box size="1 0.3 0.2" />
    </geometry>
    </collision>

    <inertial>
     <origin rpy="0 0 0" xyz="0 0 0.5"/>
      <mass value="0.03" />
      <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.4" />
  </inertial>
   </link>

<!-- camera -->
