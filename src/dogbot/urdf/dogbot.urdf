<!-- import camera -->
    <xacro:include filename="$(find dogbot)/urdf/camera.xacro"/>



  <joint name="camera_joint" type="fixed">
    <parent link="base_link"/>
    <child link="camera_link"/>
    <origin xyz="0.1 0 0.19"/>
     <xacro:web_camera prefix="camera"/>
  </joint>



<!-- front right -->
  <link name = "front_leg_right">
   <visual>
     <material name = "white"/>
     <origin rpy="0 0 0" xyz="0 0 -0.15"/>
    <geometry>
     <box size="0.1 0.1 0.3" />
    </geometry>
    </visual>

    <collision>
     <origin rpy="0 0 0" xyz="0 0 -0.15"/>
    <geometry>
     <box size="0.1 0.1 0.3" />
    </geometry>
    </collision>

    <inertial>
     <origin rpy="0 0 0" xyz="0 0 -0.15"/>
      <mass value="0.03" />
      <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.4" />
  </inertial>
   </link>


  <joint name="joint_f_r_1" type="revolute">
    <parent link="base_link"/>
    <child link="front_leg_right"/>
    <origin xyz="0.2 0.2 0.5"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1" upper="1" effort="4" velocity="2" />
  </joint>
 <transmission name="trans1">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint_f_r_1">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor1">
       <hardwareInterface>EffortJointInterface</hardwareInterface>
       <mechanismReduction>1</mechanismReduction>
    </actuator>
 </transmission>


  <link name = "front_leg_right2">
   <visual>
    <geometry>
